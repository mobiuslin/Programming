# [169] 求众数
#
class Solution(object):
    def majorityElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        nums.sort()
        len_=len(nums)
        cnt=1
        cnt_=1
        j=0
        j_max=0
        for i in range(1,len_):
            if nums[i]==nums[i-1]:
                cnt+=1
                j=i
            if nums[i]!=nums[i-1]:
                cnt=1
            if cnt_<cnt:
                cnt_=cnt
                j_max=j
        return nums[j_max]
