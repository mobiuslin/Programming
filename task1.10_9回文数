#
# [9] 回文数
#
class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        s = str(x)
        for i in range(len(s)):
            if s[i]==s[len(s)-i-1]:
                pass
            else:
                return False
        return True
